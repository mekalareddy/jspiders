package com.jspiders.java;

public class Employee 
{
	int eid;
	String ename;
	double esal;

	 Employee(int eid, String name, double sal) 
	{
		super();
		this.eid = eid;
		this.ename = name;
		this.esal = sal;
	}
	
	public String toString() 
	{
		return " ID Is : " +eid+ " NAME Is : " +ename+ " SAL Is : " + esal;
	}
}
