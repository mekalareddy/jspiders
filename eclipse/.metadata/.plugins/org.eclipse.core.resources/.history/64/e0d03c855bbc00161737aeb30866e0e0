package com.jspiders.Assignments;

public class Employee 
{
	int eid;
	String ename;
	double esal;
	String edesig;
	String email;

	public Employee(int eid, String name, double sal, String desig, String email) 
	{
		super();
		this.eid = eid;
		this.ename = name;
		this.esal = sal;
		this.edesig = desig;
		this.email = email;
	}
	
	public String toString() 
	{
		return " ID Is : " +eid+ " NAME Is : " +ename+ " SAL Is : " + esal + "DESIG IS : " +edesig+ " EMAIL IS " +email;
	}
}
